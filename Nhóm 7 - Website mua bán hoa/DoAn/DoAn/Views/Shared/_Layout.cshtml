<!DOCTYPE html>
<html>

<head>
    <meta charset="utf-8">
    @*@Styles.Render("~/Content/css")*@
    <title>Flowers Store</title>
    <link rel="stylesheet" href="~/Assets/css/Modal.css">
    @Scripts.Render("~/bundles/modernizr")
    <script src="https://code.jquery.com/jquery-3.6.0.min.js"></script>
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.5.1/jquery.min.js"></script>
    <link rel="preconnect" href="https://fonts.gstatic.com">
    <link rel="stylesheet" href="https://fonts.googleapis.com/css2?family=Poppins:wght@200;300;400;500;600;700&display=swap">
    <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/font-awesome/4.7.0/css/font-awesome.min.css">
    <link rel="icon" type="image/png" href="~/Assets/images/flower-shop.png">
    <link rel="stylesheet" href="~/Assets/fonts/themify-icons/themify-icons.css">
    <link rel="stylesheet" href="~/Assets/css/Style.css">
    <script src="https://code.jquery.com/jquery-3.6.4.min.js"></script>
    <style>
        .user-dropdown {
            position: relative;
            display: inline-block;
        }

        .dropdown-content {
            display: none;
        }

        .dropdown-content a {
            text-decoration: none;
            display: block;
        }

        .dropdown-content a:hover {
            background-color:red;
        }

        .user-dropdown:hover .dropdown-content {
            display: block;
        }
        .user-name:hover {
            color: #ff523b;
        }
    </style>
</head>

<body>
    <div class="header">
        <div class="container">
            <div class="navbar">
                <div class="logo">
                    <a href="/Home/Index"><img src="~/Assets/images/flower.jpg" width="125px"></a>
                </div>
                <nav>
                    <ul id="MenuItems">
                        <li><a href="/Home/Index">Home</a></li>
                        <li><a href="/Products/ListProducts">Products</a></li>
                        <li><a href="/Home/About">About</a></li>
                        <li><a href="/Home/Contact">Contact</a></li>
                        <li><a href="/User/Register">Account</a></li>

                    </ul>
                    @*<a href="@Url.Action("Cart", "Cart")"><i class="ti-shopping-cart" style="font-size: 44px;"></i><small>(@ViewBag.TongSoLuong)</small></a>*@
                    @Html.Action("PartialCart", "Cart")
                </nav>
                @if (Session["User"] != null)
                {
                    var user = (DoAn.Models.KhachHang)Session["User"];
                    <div class="user-dropdown">
                        <span class="user-name btn" style = "font-size: 16px; padding: 8px 16px; border-radius: 16px; margin-left: 50px; cursor: pointer;">@user.TaiKhoan</span>
                        <div class="dropdown-content">
                            @Html.ActionLink("Logout", "Logout", "User", null, new { @class = "btn", style = "font-size: 12px; padding: 8px 35px; border-radius: 16px; color: #fff; margin: 0px; position: absolute; bottom: -5px; right: 0px;"})
                        </div>
                    </div>
                }
                else
                {
                    <a href="@Url.Action("Login", "User")" class="btn" style="font-size: 16px; padding: 8px 16px; border-radius: 16px; margin-left: 50px;">
                        <i class="ti-user"></i>
                    </a>
                    @*@Html.ActionLink("<i class='ti-user'></i>", "Login", "User", null, new { @class = "btn btn-primary", style = "font-size: 16px; padding: 8px 16px; border-radius: 16px; margin-left: 50px;" })*@
                }

                <img src="~/Assets/images/menu.png" class="menu-icon" onclick="menutoggle()">
            </div>

        </div>
    </div>
    <div id="notification" class="modal fade" role="dialog">
        <div class="modal-dialog custom-dialog">
            <div class="modal-content">
                <div class="modal-header text-center">
                    <button type="button" class="close" data-dismiss="modal">&times;</button>
                    <h4 class="modal-title">Message</h4>
                </div>
                <div class="modal-body">
                    <p id="notification-message"></p>
                </div>
            </div>
        </div>
    </div>
    <div style="min-height: 748px">
        @RenderBody()
    </div>
    <!----------------------------------scroll------------------------------------->

    <button id="scrollToTopBtn"><i class="ti-arrow-up"></i></button>
    <!----------------------------------footer------------------------------------->
    <div class="footer">
        <div class="container">

            <div class="row">
                <div class="footer-col-2">
                    <img src="data:image/png;base64," />
                </div>
                <div class="footer-col-1">
                    <h3>Location</h3>
                    <p>
                        <i class="ti-location-pin"></i> 140 Lê Trọng Tấn, Tây Thạnh, Tân Phú, Thành phố Hồ Chí Minh,
                        Việt Nam
                    </p>
                    <div class="app-logo">
                        <img src="data:image/png;base64," alt="">
                        <img src="data:image/png;base64," alt="">
                    </div>
                </div>

                <div class="footer-col-3">
                    <h3>About us</h3>
                    <ul>
                        <li>Fashion</li>
                        <li>Jordan</li>
                        <li>Essentials</li>
                        <li>Trending</li>
                    </ul>
                </div>
                <div class="footer-col-4">
                    <h3>Follow us</h3>
                    <ul>
                        <li><i class="ti-facebook"></i> Facebook</li>
                        <li><i class="ti-twitter"></i> Twitter</li>
                        <li><i class="ti-instagram"></i> Instagram</li>
                        <li><i class="ti-youtube"></i> Youtube</li>
                    </ul>
                </div>

            </div>

            <hr>
            <p class="copyright">Design By Us</p>

        </div>
    </div>


    <!-----------------------------------js for toggle menu----------------------------------------------->
    <script>
        var menuItems = document.getElementById("MenuItems");

        MenuItems.style.maxHeight = "0px";
        function menutoggle() {
            if (MenuItems.style.maxHeight == "0px") {
                MenuItems.style.maxHeight = "300px";
            }
            else {
                MenuItems.style.maxHeight = "0px";
            }
        }
    </script>
    <!-----------------------------------js for button scroll----------------------------------------------->
    <script>
        // Hiển thị nút khi kéo xuống dưới một khoảng cách nhất định
        window.onscroll = function () {
            if (document.body.scrollTop > 20 || document.documentElement.scrollTop > 20) {
                document.getElementById("scrollToTopBtn").style.display = "block";
            } else {
                document.getElementById("scrollToTopBtn").style.display = "none";
            }
        };

        // Xử lý sự kiện khi click vào nút
        document.getElementById("scrollToTopBtn").addEventListener("click", function () {
            scrollToTop();
        });

        // Cuộn trang lên với hiệu ứng mượt
        function scrollToTop() {
            const currentPosition = document.documentElement.scrollTop || document.body.scrollTop;
            if (currentPosition > 0) {
                window.requestAnimationFrame(scrollToTop);
                window.scrollTo(0, currentPosition - currentPosition / 30);
            }
        }
    </script>

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)

    <script>
        function ThemGioHang(maSP) {
            $.ajax({
                type: "POST",
                url: "/Cart/ThemGioHang",
                data: { mSP: maSP },
                success: function (data) {
                    $("#notification-message").text("The bouquet has been added to the cart!");
                    $("#notification").modal("show");
                }
            });
        }
    </script>
</body>

</html>